{
  "settings": {
    "documentationRules": {
      "xmlDocumentationRequired": false
    },
    "orderingRules": {
      "usingDirectivesPlacement": "outsideNamespace",
      "systemUsingDirectivesFirst": true,
      "elementOrder": [
        "kind",
        "accessibility",
        "readonly"
      ]
    },
    "layoutRules": {
      "newlineAtEndOfFile": "require"
    },
    "namingRules": {
      "includeInferredTupleElementNames": true,
      "tupleElementNameCasing": "camelCase",
      "fieldNamePrefix": "_",
      "constantFieldNameCasing": "PascalCase",
      "readonlyFieldNamePrefix": "_",
      "staticFieldNamePrefix": "s_",
      "enumMemberCasing": "PascalCase",
      "methodNameCasing": "PascalCase",
      "propertyNameCasing": "PascalCase",
      "interfaceNamePrefix": "I"
    },
    "disabledByDefault": true,
    "ruleSets": {
      "enabledRules": [
        "SA1300",
        "SA1500",
        "SA1307",
        "SA1308",
        "SA1311",
        "SA1312"
      ]
    }
  }
}